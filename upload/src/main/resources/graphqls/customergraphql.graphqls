type Customer {
	customerId: ID!,
	customerName: String,
	dob: String,
	genderType:String,
    addressList: [Address!]
}


type Address {
	addressId: ID!,
	city: String,
	state: String,
	doorNo: String,
	streetName: String,
	customer: Customer!
}

type Query {
	findAllCustomers:[Customer]
	findCustomer(customerId: ID):Customer
}




